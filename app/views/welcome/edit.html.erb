		<textarea id='backText' type='text' class='card-input' placeholder="Back Text..."></textarea>
		<div id='addSaveContainer' class='add-mode'>
			<i class='add-icon fa fa-plus-circle fa-3x'></i>
			<i class='save-icon fa fa-save fa-3x'></i>
